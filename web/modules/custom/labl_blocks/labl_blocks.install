<?php

/**
 * @file
 * Install, update and uninstall functions for the Labl Blocks module.
 */

/**
 * Installs configuration for the Example Block paragraph.
 */
function labl_blocks_update_9001() {
  /** @var \Drupal\Core\Config\ConfigInstallerInterface $config_installer */
  $config_installer = \Drupal::service('config.installer');
  $config_installer->installDefaultConfig('module', 'labl_blocks');

  return t('Installed Example Block paragraph configuration.');
}

/**
 * Installs configuration for the CTA-7 paragraph bundle.
 */
function labl_blocks_update_9002() {
  /** @var \Drupal\Core\Config\ConfigInstallerInterface $config_installer */
  $config_installer = \Drupal::service('config.installer');
  $config_installer->installDefaultConfig('module', 'labl_blocks');

  return t('Installed CTA-7 paragraph configuration.');
}
